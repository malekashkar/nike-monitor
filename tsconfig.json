{
    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "allowJs": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "outDir": "dist",
      "sourceMap": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": ["**/**/*"]
  }
  